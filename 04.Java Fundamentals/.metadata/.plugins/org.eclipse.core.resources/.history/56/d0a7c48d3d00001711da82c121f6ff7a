
public class Test {
	public static void main(String[] args) {
		int[] array = new int[] {5, 6, 1, 2, 4, 3};
		
		for (int i : array) {
			System.out.print(i + " ");
		}
		
		System.out.println();
		bubbleSort(array);
		
		for (int i : array) {
			System.out.print(i + " ");
		}
	}
	
	public static void bubbleSort(int[] numArray) {

	    int n = numArray.length;
	    int temp = 0;

	    for (int i = 0; i < n; i++) {
	        for (int j = 1; j < (n - i); j++) {

	            if (numArray[j - 1] > numArray[j]) {
	                temp = numArray[j - 1];
	                numArray[j - 1] = numArray[j];
	                numArray[j] = temp;
	            }

	        }
	    }
	}
}
