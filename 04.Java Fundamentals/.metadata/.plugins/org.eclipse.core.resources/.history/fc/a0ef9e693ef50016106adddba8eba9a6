package _StacksAndQueues.HomeProblems;

import java.util.Scanner;

public class _08BalancedParantheses {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		//String inputParentheses = scanner.nextLine();
		char[] parenthesesArray = scanner.next().toCharArray();
		
		for (int i = 0; i < parenthesesArray.length; i++) {
			System.out.print(parenthesesArray[i]);
		}
		
	}
}
