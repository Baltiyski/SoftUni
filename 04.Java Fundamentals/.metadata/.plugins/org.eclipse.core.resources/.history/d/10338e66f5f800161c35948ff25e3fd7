package _01DefiningClasses.Lab;

import java.util.HashMap;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		HashMap<Integer, BankAccount> accounts = new HashMap<>();
		
		String command =scanner.nextLine();
		while (!command.equals("End")) {
			String[] commandArg = command.split("\\s+");
			String commandType = commandArg[0];
			switch (commandType) {
			case "Create":

				break;
			case "Deposit":
				break;
			case "Withdraw":
				break;
			case "Print":
				break;
			default:
				break;
			}
			
			command = scanner.nextLine();
		}
	}
	
	private static void executeCreateCommand(HashMap<Integer, BankAccount> account, String commandArg) {
		int id = Integer.valueOf(commandArg);
		if (!account.containsKey(id)) {
			System.out.println("Account already exists");
		} else {
			BankAccount accounts = new BankAccount();
			accounts.setId(id);
			account.put(key, accounts);
		}
	}
}
